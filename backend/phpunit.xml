<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="bootstrap.php" verbose="true" cacheResult="true" cacheResultFile="./Tests/Output/.phpunit.result.cache">
    
    <testsuites>
        <testsuite name="Unit Tests">
            <directory>./Tests</directory>
        </testsuite>
    </testsuites>

    <coverage>
        <include>
            <directory suffix=".php">./App</directory>
        </include>
    </coverage>
</phpunit>
